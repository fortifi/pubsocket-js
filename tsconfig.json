{
  "compilerOptions": {
    "target": "ES2017",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client",
      "node"
    ],
    "resolveJsonModule": true
  },
  "typeRoots": [
    "node_modules/@types",
    "./src/types"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "builds"
  ]
}
